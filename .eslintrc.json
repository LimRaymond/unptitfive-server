{
    "env": {
        "browser": true,
        "es2020": true
    },
    "extends": ["eslint:recommended", "airbnb-base"],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "indent": ["error", 4],
        "no-console": "off",
        "no-shadow": "off",
        "no-underscore-dangle": "off",
        "arrow-body-style": "off",
        "no-param-reassign": "off"
    }
}
